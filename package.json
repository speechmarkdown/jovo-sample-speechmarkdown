{
  "name": "jovo-sample-speechmarkdown",
  "version": "1.0.0",
  "description": "A sample voice app that works with the Jovo Framework and uses Jovo Speech Markdown plugin",
  "main": "src/index.js",
  "dependencies": {
    "jovo-db-filedb": "^2.0.2",
    "jovo-framework": "^2.2.13",
    "jovo-platform-alexa": "^2.0.2",
    "jovo-platform-googleassistant": "^2.0.2",
    "jovo-plugin-debugger": "^2.0.2",
    "jovo-plugin-speechmarkdown": "^1.0.0-beta.0"
  },
  "devDependencies": {
    "gulp": "^4.0.0",
    "gulp-install": "^1.1.0",
    "gulp-run-command": "0.0.9",
    "gulp-zip": "^4.2.0",
    "jest": "^23.6.0"
  },
  "scripts": {
    "tsc": "node -v",
    "test": "jest",
    "bundle": "gulp --gulpfile node_modules/jovo-framework/gulpfile.js --cwd ./",
    "start": "cd src && node index.js --webhook",
    "launch": "npm start -- --launch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speechmarkdown/jovo-sample-speechmarkdown.git"
  },
  "author": "Jovo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/speechmarkdown/jovo-sample-speechmarkdown/issues"
  },
  "homepage": "https://github.com/speechmarkdown/jovo-sample-speechmarkdown#readme"
}
